{
	"name": "my-vscode-extensions",
	"displayName": "my-vscode-extensions",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "my-vscode-extensions.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "my-vscode-extensions.func-comment",
				"title": "Function comment"
			}
		],
		"configuration": {
			"type": "object",
			"title": "my-vscode-extensions",
			"properties": {
				"my-extensions.comment.function.template": {
					"type": "array",
					"default": [
						"/// @1\t",
						"/// @2\t",
						"/// @3\t"
					],
					"description": "@1: @brief @2: @param @3: @retval"
				},
				"my-extensions.comment.function.display-void.@param": {
					"type": "boolean",
					"default": true
				},
				"my-extensions.comment.function.display-void.@retval": {
					"type": "boolean",
					"default": true
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.85.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8.54.0",
		"typescript": "^5.3.2",
		"ts-loader": "^9.5.1",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"@vscode/test-cli": "^0.0.4",
		"@vscode/test-electron": "^2.3.8"
	}
}